{% extends "_main.jinja" %}
{% block title %} LOGIN {% endblock %}

{% block main %} 
<form action="{{ url_for("view_user") }}" method="post">
	{{form.csrf_token}}
	<p>
		{{ form.username.label }}
		{{ form.username() }}
	
	</p>
	<p>
		{{ form.password.label }}
		{{ form.password() }}
	</p>
	<input type="submit" name="login">
</form>
{% endblock %}